# Basic paths that are common to all my machines

alias home="cd ~"
alias dl="cd ~/Downloads"
alias desk="cd ~/Desktop"
alias doc="cd ~/Documents"
alias docs="cd ~/Documents"

alias dot="cd ~/.dotfiles"

#alias gd="cd ~/GoogleDrive"
#alias gdrive="cd ~/GoogleDrive"

export PATH="$HOME/.dotfiles/git/bin:$PATH"
export PATH="$HOME/GoogleDrive/Projects/QMix/bin:$PATH"
